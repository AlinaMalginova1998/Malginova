public class ProstyeChisla 
{
	public static void main(String[] args) 
	{
		for (int i=1;i<=100;i++)
		{
			if (Check(i))
				System.out.println("Prostoye chislo - "+i);	
			else 
				System.out.println("Ne prostoye chislo - "+i);	
		}
	}
	public static boolean Check(int n)
	{
		for (int j=2;n>j;j++)
		{
			if (n%j==0)
			{
				return false;
			}
		}
		return true;
	}
}
